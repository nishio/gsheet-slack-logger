# Slack設定
INPUT_SLACKTOKEN=xoxb-your-token-here
INPUT_TIMEZONE=Asia/Tokyo

# Google Drive設定
INPUT_GOOGLECLIENTEMAIL=your-service-account@example.iam.gserviceaccount.com
INPUT_GOOGLEPRIVATEKEY=-----BEGIN PRIVATE KEY-----\nYourPrivateKeyHere\n-----END PRIVATE KEY-----
INPUT_FOLDERID=your-folder-id-here

# オプション設定
INPUT_AUTOJOIN=true
INPUT_SKIPCHANNELS=
INPUT_USELATESTFILE=false
INPUT_BACKUPWITHDATE=false
INPUT_LASTDAYS=30
# INPUT_YEAR=2023
# INPUT_MONTH=1

# Node.js互換性設定
# Node.js v14以降でOpenSSLエラーが発生する場合に必要
NODE_OPTIONS=--openssl-legacy-provider
